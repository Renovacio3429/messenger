{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2020",
    "declaration": false,
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "sourceMap": true,
    "esModuleInterop" : true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": "./src"
  }
}
